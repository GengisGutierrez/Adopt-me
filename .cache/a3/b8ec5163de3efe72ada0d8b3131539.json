umn":15}},{"generated":{"line":138,"column":36},"source":"Details.js","original":{"line":63,"column":50}},{"generated":{"line":139,"column":0},"source":"Details.js","original":{"line":64,"column":4}},{"generated":{"line":140,"column":0},"source":"Details.js","original":{"line":64,"column":4}},{"generated":{"line":141,"column":0},"source":"Details.js","original":{"line":65,"column":8}},{"generated":{"line":141,"column":25},"source":"Details.js","original":{"line":65,"column":9}},{"generated":{"line":141,"column":47},"source":"Details.js","original":{"line":65,"column":8}},{"generated":{"line":142,"column":0},"source":"Details.js","original":{"line":65,"column":8}},{"generated":{"line":143,"column":0},"source":"Details.js","original":{"line":65,"column":8}},{"generated":{"line":144,"column":0},"source":"Details.js","original":{"line":66,"column":12}},{"generated":{"line":144,"column":27},"source":"Details.js","original":{"line":66,"column":13}},{"generated":{"line":144,"column":44},"source":"Details.js","original":{"line":66,"column":12}},{"generated":{"line":145,"column":0},"source":"Details.js","original":{"line":65,"column":8}},{"generated":{"line":146,"column":0},"source":"Details.js","original":{"line":64,"column":4}},{"generated":{"line":147,"column":0},"source":"Details.js","original":{"line":69,"column":1}}],"sources":{"Details.js":"import { Component } from \"react\";\r\nimport { withRouter } from \"react-router-dom\";\r\nimport Carousel from \"./Carousel\";\r\nimport ErrorBoundary from \"./ErrorBoundary\";\r\nimport ThemeContext from \"./ThemeContext\";\r\nimport Modal from './Modal'\r\n\r\nclass Details extends Component {\r\n    state = { loading: true, showModal: false };\r\n    async componentDidMount() {\r\n        const res = await fetch(\r\n          `http://pets-v2.dev-apis.com/pets?id=${this.props.match.params.id}`\r\n        );\r\n        const json = await res.json();\r\n        this.setState(Object.assign({ loading: false }, json.pets[0]));\r\n      }\r\n    \r\n    toggleModal = () => this.setState({ showModal: !this.state.showModal });\r\n    adopt = () => (window.location = \"http://bit.ly/pet-adopt\");\r\n\r\n    render () {\r\n\r\n        if(this.state.loading){\r\n            return <h2>Loading...</h2>\r\n        }\r\n\r\n        const { animal, breed, city, state, description, name, images, showModal} = this.state; \r\n\r\n        return (\r\n            <div className=\"details\">\r\n                <Carousel images={images}/>\r\n                <h1>{name}</h1>\r\n                <h2>{`${animal} - ${breed} - ${city}, ${state}`}</h2>\r\n                <ThemeContext.Consumer>\r\n                    {([theme]) => (\r\n                        <button onClick={this.toggleModal} style={{ backgroundColor: theme }}>\r\n                            Adopt {name}\r\n                        </button>\r\n                    )}\r\n                </ThemeContext.Consumer>\r\n                \r\n                <p>{description}</p>\r\n                {\r\n                    showModal ? (\r\n                      <Modal>\r\n                        <div>\r\n                          <h1>Would you like to adopt {name}?</h1>\r\n                          <div className=\"buttons\">\r\n                            <button onClick={this.adopt}>Yes</button>\r\n                            <button onClick={this.toggleModal}>No</button>\r\n                          </div>\r\n                        </div>\r\n                      </Modal>\r\n                    ) : null\r\n                }\r\n            </div>\r\n        )\r\n    }\r\n}\r\n\r\nconst DetailsWithRouter = withRouter(Details)\r\n\r\nexport default function DetailsWithErrorBoundary(){\r\n    return (\r\n        <ErrorBoundary>\r\n            <DetailsWithRouter/>\r\n        </ErrorBoundary>\r\n    )\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"a9865af4d5dfc32d1a636bf82339392b","cacheData":{"env":{}}}